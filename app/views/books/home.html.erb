<% provide(:title, "Home") %>
<%= @user.email %>


<div class="container">
  <div class="row">

    <div class="col-lg-8 col-lg-offset-2">

      <%= form_for(:books, url: home_path) do |f| %>

        <%= f.label :search %>
        <%= f.text_field :q, class: "form-control"%>

        <%= f.submit "Search", class:"btn btn-primary"%>
      <% end %>

      <%= render 'search'%>


    </div>

  </div>
</div>
